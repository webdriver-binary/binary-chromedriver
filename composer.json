{
    "name": "webdriver-binary/binary-chromedriver",
    "type": "composer-plugin",
    "license": "MIT",
    "description": "Utility for downloading Chrome Driver binary for current os/platform",
    "require": {
        "php": ">=7.2",
        "composer-plugin-api": "^1.0 | ^2.0",
        "webdriver-binary/webdriver-binary-downloader": "^3.0.1"
    },
    "replace": {
        "lbaey/chromedriver": "*",
        "vaimo/binary-chromedriver": "*",
        "lanfest/binary-chromedriver": "*"
    },
    "support": {
        "source": "https://github.com/webdriver-binary/binary-chromedriver",
        "docs": "https://github.com/webdriver-binary/binary-chromedriver",
        "issues": "https://github.com/webdriver-binary/binary-chromedriver/issues"
    },
    "keywords": [
        "chromedriver",
        "chrome automation",
        "testing",
        "test-automation",
        "composer plugin",
        "composer extension",
        "browser driver",
        "webdriver",
        "acceptance testing",
        "browser testing",
        "downloader",
        "binary downloader",
        "binary"
    ],
    "autoload": {
        "psr-4": {
            "WebdriverBinary\\ChromeDriver\\": "src"
        }
    },
    "extra": {
        "class": "WebdriverBinary\\ChromeDriver\\Plugin"
    }
}
